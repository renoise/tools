						   <Param value="CC#28" type="togglebutton" size="1" orientation="vertical" name="" aspect="0.6" maximum="127" minimum="0"/>
						   <Param value="CC#28" type="togglebutton" size="1" orientation="vertical" name="" aspect="0.6" maximum="127" minimum="0"/>
