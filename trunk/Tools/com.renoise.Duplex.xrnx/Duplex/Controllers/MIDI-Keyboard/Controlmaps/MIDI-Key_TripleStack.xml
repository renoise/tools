<Device>
  <Name>TripleKeys MIDI-Keyboard</Name>
  <Author>Renoise Team</Author>
  <Description>Stacked MIDI-Keyboards: here, you can control three instruments using one master keyboard. 
The lower keyboard has custom velocity (full velocity, no matter how hard you press)</Description>
  <Parameters>
    <Row>
		<Column>
			<Row>
				<Column>
					<Group name="Volume1" orientation="vertical">
					   <Param value="CC#7" type="dial" size="1" orientation="vertical" name="Volume" maximum="127" minimum="0"/>
					   <Param value="CC#8" type="togglebutton" size="1" orientation="vertical" name="" aspect="0.7" maximum="127" minimum="0"/>
					 </Group>
					 <Group name="Octave1" orientation="vertical">
					   <Param value="CC#9" type="togglebutton" size="1" orientation="vertical" name="OctUp" aspect="0.6" maximum="127" minimum="0"/>
					   <Param value="CC#10" type="togglebutton" size="1" orientation="vertical" name="OctDn" aspect="0.6" maximum="127" minimum="0"/>
					   <Param value="CC#10" type="togglebutton" size="1" name="OctSync" aspect="0.7" maximum="127" minimum="0"/>
					 </Group>
				</Column>
				<Group name="PB1" >
					<Param value="PB|Ch1" type="fader" size="4" orientation="vertical" name="Master" maximum="127" minimum="0"/>
				</Group>
				<Group name="MOD1" >
					<Param value="CC#10" type="fader" size="4" orientation="vertical" name="Master" maximum="127" minimum="0"/>
				</Group>
				<Group name="CP1" >
					<Param value="CP|Ch1" type="fader" size="4" orientation="vertical" name="Master" maximum="127" minimum="0"/>
				</Group>
				<Group name="Keyboard1" orientation="horizontal" >
					<Param value="|Ch1" type="keyboard" name="TwoOctaves" size="1" velocity_enabled="true" aspect="1" range="48" maximum="127" minimum="0"/>
				</Group>
			</Row>
			<Row>
				<Column>
					<Group name="Volume2" orientation="vertical">
					   <Param value="CC#27" type="dial" size="1" orientation="vertical" name="Volume" maximum="127" minimum="0"/>
